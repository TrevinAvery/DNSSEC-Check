{
	"name": "DNSSEC Check",
	"description": "Uses DoH to check if the current address supports DNSSEC",
	"version": "0.0.1",
	"manifest_version": 3,
	"permissions": [
		"tabs",
		"storage"
	],
	"host_permissions": [
		"https://*/"
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
		  "16": "/images/default/16.png",
		  "32": "/images/default/32.png",
		  "48": "/images/default/48.png",
		  "128": "/images/default/128.png"
		}
	},
	"icons": {
	  "16": "/images/default/16.png",
	  "32": "/images/default/32.png",
	  "48": "/images/default/48.png",
	  "128": "/images/default/128.png"
	}
}